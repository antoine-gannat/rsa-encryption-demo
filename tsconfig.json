{
  "compilerOptions": {
    "module": "CommonJs",
    "target": "es2016",
    "outDir": "./lib/",
    "rootDir": "./src",
    "declaration": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": ["./src/**/*"]
}
